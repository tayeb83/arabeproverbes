<h1>تسجيل الدخول</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :"اسم المستخدم" %><br />
    <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :"كلمة السر" %><br />
    <%= password_field_tag :password %>
  </p>
  <p class="btn btn-large btn-primary"><%= submit_tag "تسجيل الدخول" %></p>
<% end %>



