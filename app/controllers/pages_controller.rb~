class PagesController < ApplicationController
  def home
  @title = "الرئيسية"
  @proverbe = current_user.proverbes.build if signed_in?
  @feed_items = current_user.feed.paginate(:page=> params[:page])
  end

  def contact
  @title = "اتصل بنا"
  end

  def terms
  @title = ""
  end

  def help
  @title = "مساعدة"	
  end

  def about
 @title = "عنا"
  end

  def privacy
  end
end
